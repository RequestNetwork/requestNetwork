{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default"
    }
  },
  "targetDefaults": {
    "lint:check": {
      "dependsOn": ["^lint:check"],
      "cache": true
    },
    "build": {
      "dependsOn": ["^build", "lint:check"],
      "cache": true
    },
    "test": {
      "dependsOn": ["build"],
      "cache": true
    },
    "clean": {
      "dependsOn": ["^clean"],
      "cache": true
    }
  }
}
