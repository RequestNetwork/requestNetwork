{
  "name": "@requestnetwork/toolbox",
  "version": "0.1.0",
  "private": true,
  "description": "Toolbox for Request Network.",
  "keywords": [
    "requestnetwork",
    "toolbox"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/toolbox#readme",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "request-toolbox": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "shx rm -rf dist",
    "lint": "tslint --project . && eslint \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "prepare": "yarn run build",
    "cli": "cross-env NODE_ENV=development ts-node src/cli.ts"
  },
  "dependencies": {
    "@requestnetwork/epk-signature": "0.5.0",
    "@requestnetwork/request-client.js": "0.5.0",
    "@requestnetwork/types": "0.5.0",
    "axios": "0.18.1",
    "yargs": "14.0.0"
  },
  "devDependencies": {
    "cross-env": "5.2.1",
    "eslint": "5.13.0",
    "eslint-plugin-spellcheck": "0.0.11",
    "eslint-plugin-typescript": "0.14.0",
    "husky": "2.4.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "shx": "0.3.2",
    "tslint": "5.12.1",
    "typescript": "3.3.1"
  }
}
