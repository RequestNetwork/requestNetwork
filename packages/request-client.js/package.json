{
  "name": "@requestnetwork/request-client.js",
  "version": "0.40.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Client side library for Request Network.",
  "keywords": [
    "requestnetwork",
    "request-client.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/request-client.js#readme",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s build:commonjs build:umd",
    "build:commonjs": "tsc -b tsconfig.build.json",
    "build:umd": "webpack",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "docs": "shx rm -rf ./docs && compodoc -p tsconfig.json --output docs --disablePrivate --gaID UA-105153327-8",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prepare": "yarn run build",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@requestnetwork/advanced-logic": "0.35.0",
    "@requestnetwork/currency": "0.9.0",
    "@requestnetwork/data-access": "0.27.0",
    "@requestnetwork/data-format": "0.10.0",
    "@requestnetwork/epk-signature": "0.5.35",
    "@requestnetwork/multi-format": "0.15.10",
    "@requestnetwork/payment-detection": "0.36.0",
    "@requestnetwork/request-logic": "0.26.10",
    "@requestnetwork/smart-contracts": "0.29.0",
    "@requestnetwork/transaction-manager": "0.27.0",
    "@requestnetwork/types": "0.36.0",
    "@requestnetwork/utils": "0.36.0",
    "axios": "0.27.2",
    "ethers": "5.5.1",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.11",
    "@types/jest": "26.0.13",
    "amd-loader": "0.0.8",
    "axios-mock-adapter": "1.19.0",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "jest": "26.4.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "shx": "0.3.2",
    "source-map-support": "0.5.19",
    "terser-webpack-plugin": "4.2.3",
    "ts-jest": "26.3.0",
    "ts-loader": "8.4.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "4.2.0",
    "webpack-cli": "3.3.12"
  },
  "gitHead": "6155223cfce769e48ccae480c510b35b4f54b4d0"
}
