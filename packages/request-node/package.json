  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "dist/bin",
  "bin": {
    "request-node": "dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "build:watch": "tsc -b tsconfig.build.json --watch",
