{
  "name": "@requestnetwork/usage-examples",
  "version": "0.21.0",
  "private": true,
  "description": "Usage examples of Request Network.",
  "keywords": [
    "requestnetwork",
    "usage-examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/usage-examples#readme",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "lint": "tslint --project . && eslint \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "start": "ts-node src/request-client-js-declarative-request.ts && ts-node src/request-client-js-erc20-request.ts && ts-node src/request-logic-clear-request.ts && ts-node src/request-logic-encrypted-request.ts",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@requestnetwork/data-access": "0.21.0",
    "@requestnetwork/epk-decryption": "0.3.22",
    "@requestnetwork/epk-signature": "0.5.23",
    "@requestnetwork/multi-format": "0.13.0",
    "@requestnetwork/payment-processor": "0.28.0",
    "@requestnetwork/request-client.js": "0.28.0",
    "@requestnetwork/request-logic": "0.24.0",
    "@requestnetwork/transaction-manager": "0.24.0",
    "@requestnetwork/types": "0.27.0",
    "@requestnetwork/utils": "0.26.0",
    "ethers": "4.0.48"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "4.1.1",
    "eslint": "7.9.0",
    "eslint-plugin-spellcheck": "0.0.17",
    "eslint-plugin-typescript": "0.14.0",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  }
}
