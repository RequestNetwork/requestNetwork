{
  "name": "@requestnetwork/usage-examples",
  "version": "0.5.2",
  "private": true,
  "description": "Usage examples of Request Network.",
  "keywords": [
    "requestnetwork",
    "usage-examples"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RequestNetwork/requestNetwork.git"
  },
  "homepage": "https://github.com/RequestNetwork/requestNetwork/tree/master/packages/usage-examples#readme",
  "bugs": {
    "url": "https://github.com/RequestNetwork/requestNetwork/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc -b",
    "clean": "shx rm -rf dist tsconfig.tsbuildinfo",
    "lint": "tslint --project . && eslint \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "start": "ts-node src/request-client-js-declarative-request.ts && ts-node src/request-client-js-erc20-request.ts && ts-node src/request-logic-clear-request.ts && ts-node src/request-logic-encrypted-request.ts",
    "prepare": "yarn run build"
  },
  "dependencies": {
    "@requestnetwork/data-access": "0.5.4",
    "@requestnetwork/epk-decryption": "0.3.5",
    "@requestnetwork/epk-signature": "0.5.6",
    "@requestnetwork/multi-format": "0.2.3",
    "@requestnetwork/request-client.js": "0.11.0",
    "@requestnetwork/request-logic": "0.8.2",
    "@requestnetwork/transaction-manager": "0.8.3",
    "@requestnetwork/types": "0.10.0",
    "@requestnetwork/utils": "0.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "1.2.0",
    "eslint": "5.13.0",
    "eslint-plugin-spellcheck": "0.0.11",
    "eslint-plugin-typescript": "0.14.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "ts-node": "8.5.4",
    "tslint": "5.12.1",
    "typescript": "3.7.2"
  }
}
